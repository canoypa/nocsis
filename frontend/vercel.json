{
  "framework": null,
  "buildCommand": null,
  "outputDirectory": "build/web",
  "installCommand": null,
  "devCommand": null,

  "rewrites": [
    {
      "source": "/__/auth/:path*",
      "destination": "https://class-clock-40088.firebaseapp.com/__/auth/:path*"
    },
    { "source": "/(.*)", "destination": "/" }
  ],

  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cross-Origin-Embedder-Policy", "value": "credentialless" },
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
      ]
    }
  ]
}
